<?xml version="1.0" encoding="UTF-8"?>
<framework xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas/frameworkDescriptionVersion1.1.xsd" name="{{ name }}" invoke="app/console" alias="{{ alias }}" enabled="true" version="1">
{% for command in commands %}
<command>
    <name>{{ command.name }}</name>
    <help>{{ command.help }}</help>
    <params>{{ command.params }}</params>
</command>
{% endfor %}
</framework>